+++
theme_color: orange
nav: admin_users
breadcrumbs_top: True
+++
{{tabs}}

<div class="row">
  <div class="col-md-12">
    <br>
    <form class="form-inline pull-right" role="pagination_options">
      {{page.options}}
    </form>
    <br>
    <br>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover table-condensed">
      <thead>
        <th>Name</th>
        <th>Rev</th>
        <th>Created</th>
        <th>Status</th>
      </thead>
      <tbody>
        {% for image in page.pail %}
          <tr>
            <td><a href="/admin/images/{{image.id}}">{{image.name}}</a></td>
            <td>{{image.rev}}</td>
            <td>{{image.formated_created}}</td>
            <td></td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    {{page.paginate}}
  </div>
</div>
